			function seite_aufschlagen(element, id) {
				var index = element.selectedIndex;
				var page = element.options[index].id;
				self.location.href="show?id="+id+"&page="+page;
			}

			$(document).ready(function() {
				moveSkyscraper();
			});

			$(window).resize(function() {
				moveSkyscraper();
			});
			
			$(window).scroll(function() {
			    var containerOffset = $("#container").offset();
			    var containerTop = containerOffset.top;
			    var containerLeft = containerOffset.left;
			    var containerWidth = $("#container").width();
			    var skyscraperWidth = containerLeft + containerWidth + 15;
			    
			    var windowTop = $(window).scrollTop();
			    
			    var diff = containerTop - windowTop;
			    
			    if (diff < 0) {
			        $("#skyscraper").addClass("fixedPosition");
			        $('#skyscraper').css({left: skyscraperWidth});
			        $("#container").addClass("paddingTop");
			    } else {
			        //$("#skyscraper").removeClass("fixedPosition");
			        $('#skyscraper').css({left: skyscraperWidth});
			        $("#container").removeClass("paddingTop");
			    }
			});	
			
			function moveSkyscraper() {
			    var containerOffset = $("#container").offset();
			    var containerTop = containerOffset.top;
			    var containerLeft = containerOffset.left;
			    var containerWidth = $("#container").width();
			    var skyscraperWidth = containerLeft + containerWidth + 15;
			    
		        $("#skyscraper").addClass("fixedPosition");
		        $('#skyscraper').css({left: skyscraperWidth});
			} 
	
