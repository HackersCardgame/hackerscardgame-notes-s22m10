.bitmovinplayer-container object, .bitmovinplayer-container embed, .bitmovinplayer-container img {
    height: 100%;
}

.bitmovinplayer-container, .bitmovinplayer-ad-container {
    width:100%;
    height:100%;
    background-color: black;
    color: #ffffff;
